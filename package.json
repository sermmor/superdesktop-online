{
    "name": "superdesktop-online",
    "version": "1.0.5",
    "main": "dist/index.js",
    "license": "MIT",
    "author": "Sergio",
    "scripts": {
        "prestart": "npm run prepare-svg",
        "start": "webpack-dev-server --watch",
        "build:webpack": "webpack",
        "prepare-svg": "npm run add-missing-size && npm run svg-optimize",
        "add-missing-size": "ts-node ./scripts/add-missing-size.ts --folder=./assets",
        "svg-optimize": "ts-node ./scripts/svg-optimize.ts",
        "prebuild": "npm run prepare-svg",
        "build:copy:assets": "cpy assets/normal/*.svg dist/assets",
        "build:copy:fonts": "cpy fonts/*.* dist/fonts",
        "build:copy:scenes": "cpy src/scenes/*.* dist/scenes",
        "build": "rimraf ./dist && tsc --project ./tsconfig.json && npm run build:copy:assets && npm run build:copy:fonts && npm run build:copy:scenes",
        "postbuild:removeAssets": "rimraf dist/assets"
    },
    "dependencies": {
        "webfront-graphics-core-engine": "../webfront-graphics-core-engine",
        "webfontloader": "1.6.28"
    },
    "devDependencies": {
        "@types/glob": "5.0.35",
        "@types/mkdirp": "^0.5.2",
        "@types/node": "^9.6.50",
        "@types/superagent": "^4.1.3",
        "@types/svgo": "^1.3.2",
        "@types/webfontloader": "1.6.29",
        "@types/yargs": "^10.0.1",
        "awesome-typescript-loader": "5.2.0",
        "copy-webpack-plugin": "4.5.2",
        "core-js": "^2.6.9",
        "cpy-cli": "^1.0.1",
        "glob": "7.1.3",
        "html-webpack-plugin": "3.2.0",
        "jsonfile": "^5.0.0",
        "mkdirp": "^0.5.1",
        "raw-loader": "0.5.1",
        "rimraf": "^2.6.3",
        "superagent": "^5.1.0",
        "svgo": "^1.3.2",
        "ts-node": "^4.1.0",
        "typescript": "3.5.3",
        "uglifyjs-webpack-plugin": "1.3.0",
        "webpack": "4.16.5",
        "webpack-cli": "3.1.0",
        "webpack-dev-server": "3.1.5",
        "yargs": "^11.0.0"
    }
}
